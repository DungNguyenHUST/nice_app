<%if @is_post_comment%>
$('#post_show_comment_index_<%=@post.id%>').html('<%= j render partial: "post_comments/js_form", locals: {post: @post} %>');
<%else%>
$('#post_reply_comment_index_<%=@post_comment.id%>').html('<%= j render partial: "post_comments/js_form", locals: {post_comment: @post_comment} %>');
<%end%>